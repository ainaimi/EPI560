	j2 = {BMC pediatrics},
	keywords = {Adolescent Advertising as Topic Canada/epidemiology Child Child Behavior/*psychology Cluster Analysis Female Humans Male Marketing Mass Media Smoking/*epidemiology *Social Perception Sports Students/*psychology Visual Perception},
