	volume = {192},
	year = {2005},
	bdsk-url-1 = {http://dx.doi.org/10.1016/j.ajog.2005.02.015}}

@article{Goldenberg2003,
	abstract = {In summary, there is little question that intrauterine and some extrauterine infections play important roles in the etiology of early, spontaneous, preterm labor and PROM. Disappointing are the mixed results from various treatment attempts, usually with antibiotics, to reduce the preterm birth rate. Clearly, a better understanding of the pathways leading from infection to preterm birth will be necessary to develop effective interventions to reduce infection-related preterm delivery. Research must also address the question of individual susceptibility to infections and the influence of other exposures that may moderate the association between infection and preterm birth.},
	author = {Goldenberg, Robert L and Culhane, Jennifer Flatow},
	date-added = {2015-06-27 00:36:44 +0000},
