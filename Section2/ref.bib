@article{Kallan1992,
	abstract = {This study examines the magnitude and shape of the interpregnancy interval (IPI) effect on three pregnancy outcomes: preterm low birthweight (PRETERM-LBW), intrauterine growth-retardation low birthweight (IUGR-LBW), and fetal loss (LOSS). A multinomial logistic regression model is estimated, based on data from the 1988 National Survey of Family Growth which contains pregnancy histories. The results indicate that both short and long intervals raise the risk of IUGR-LBW and LOSS, net of sociodemographic and behavioral variables, but IPI effects on PRETERM-LBW are not clear.},
	author = {Kallan, J E},
	date-added = {2015-06-27 00:36:44 +0000},
	date-modified = {2015-06-27 00:36:44 +0000},
	journal = {Soc Biol},
	keywords = {Biology; *Birth Spacing; *Birth Weight; Body Weight; Demographic Factors; Family Planning; *Fetal Death; Fetus; *Gestational Age; Mortality; Physiology; Population; Population Dynamics; Pregnancy; Reproduction; Research Methodology; *Retrospective Studies; Sampling Studies; Studies; *Surveys},
	month = {Fall-Winter},
	note = {PIP: TJ: SOCIAL BIOLOGY.},
	number = {3-4},
	oab = {PIP: To determine the effect of the interpregnancy interval on 1) preterm low birth weight ( 37 weeks, 5 pounds, 9 ounces), 2) intrauterine growth retardation low birth weight ( 37 weeks, 5 pounds, 9 ounces), and 3) fetal loss (early and late, but excluding ectopic pregnancies), data were analyzed from the US national survey of family growth in 1988 which represents noninstitutionalized US women aged 15-44. The final sample size included 104 pregnancies. A multivariate model (established by logistic regression) was used to control for mostly demographic variables (race, education, age at pregnancy, pregnancy order, prior pregnancy outcome, and smoking). The results how that short and long interpregnancy intervals increase the risk of both intrauterine growth retardation low birth weight and fetal loss. The data may have been affected by the retrospective nature of the study which depends on accuracy of recall, which is associated with underreporting and inaccurate dating.},
	oabl = {eng},
	oto = {PIP},
	pages = {231--245},
	title = {Effects of interpregnancy intervals on preterm birth, intrauterine growth retardation, and fetal loss},
	volume = {39},
	year = {1992}}

@article{Zhu2001,
	abstract = {OBJECTIVE: We evaluated interpregnancy interval in relation to adverse perinatal outcomes and whether the relationship differed by race. STUDY DESIGN: We analyzed the vital statistics data for multiparous white and black women in Michigan who delivered a singleton live birth during the period 1993 through 1998, using stratified and logistic regression techniques. RESULTS: Among women of both races, the risk for delivering low birth weight, premature, and small-for-gestational-age birth was lowest if the interpregnancy interval was 18 to 23 months. In comparison, among white women, the odds ratios for the 3 outcomes were 1.5, 1.3, and 1.3, respectively, if the interval was <6 months, and 1.9, 1.4, and 1.7, respectively, if the interval was > or =120 months, controlling for other factors. Similarly, among black women, the odds ratios were 1.5, 1.2, and 1.3, respectively, if the interval was <6 months, and 1.6, 1.3, and 1.4, respectively, if the interval was > or =120 months. CONCLUSION: An interpregnancy interval of 18 to 23 months is associated with the lowest risk for adverse perinatal outcomes among both white and black women.},
	author = {Zhu, B P and Haines, K M and Le, T and McGrath-Miller, K and Boulton, M L},
	date-added = {2015-06-27 00:36:44 +0000},
	date-modified = {2015-06-27 00:36:44 +0000},
	doi = {10.1067/mob.2001.118307},
	journal = {Am J Obstet Gynecol},
	month = {Dec},
	number = {6},
	pages = {1403--1410},
	title = {Effect of the interval between pregnancies on perinatal outcomes among white and black women},
	volume = {185},
	year = {2001},
	bdsk-url-1 = {http://dx.doi.org/10.1067/mob.2001.118307}}

@article{Khoshnood1998,
	abstract = {The authors studied the effects and population-level impact of short (≤12 months) interpregnancy intervals on the risks for low (<2.5 kg) birth weight and preterm (<37 weeks) delivery of liveborn singleton infants to US African American, Mexican, Native American, non-Hispanic white, and Puerto Rican mothers (n = 4,841,418) from 1989 to 1991. Statistical analyses were done by using the Mantel-Haenszel correlation statistic chi-square test and logistic regression. The proportion of livebirths associated with ≤12-month interpregnancy intervals was the lowest among non-Hispanic whites (18.5%, 95% confidence interval 18.5--18.5) and the highest among Native Americans (29.7%, 95% confidence interval 29.2--30.2). As compared with mothers with >12-month intervals, mothers with <6-month intervals had an approximately 50% to 80% increased risk of very low (<1.5 kg) birth weight delivery and a 30% to 90% increased risk of very preterm (<32 weeks) delivery. Logistic regression analyses showed that the adverse effects of short intervals were reduced by about 10% but remained for the most part significant after controlling for potential confounding by maternal age, education, parity, marital status, prenatal care, smoking, and previous preterm delivery. Am J Epidemiol 1998; 148:798--805.},
	author = {Khoshnood, Babak and Lee, Kwang-Sun and Wall, Stephen and Hsieh, Hui-lung and Mittendorf, Robert},
	date-added = {2015-06-27 00:36:44 +0000},
	date-modified = {2015-06-27 00:36:44 +0000},
	journal = {Am J Epidemiol},
	number = {8},
	pages = {798-805},
	title = {Short Interpregnancy Intervals and the Risk of Adverse Birth Outcomes among Five Racial/Ethnic Groups in the United States},
	volume = {148},
	year = {1998},
	bdsk-url-1 = {http://aje.oxfordjournals.org/content/148/8/798.abstract}}

