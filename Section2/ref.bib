	author = {Wang, Aolin and Arah, Onyebuchi A},
	date-added = {2015-11-06 11:47:27 +0000},
	date-modified = {2015-11-06 11:48:48 +0000},
	doi = {10.1007/s10654-015-0100-z},
	journal = {Eur J Epidemiol},
	keywords = {Effect decomposition; G-computation; Mediation analysis; Parametric g-formula; Simulation},
	month = {Nov},
