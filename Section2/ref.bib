	keywords = {*Birth Weight Cross-Sectional Studies *Embryonic and Fetal Development/physiology Female Fetal Weight/physiology Humans Longitudinal Studies *Models, Biological Pregnancy Pregnancy Trimester, Third/physiology *Ultrasonography, Prenatal Weight Gain/physiology},
	note = {Fry, A G Bernstein, I M Badger, G J Comparative Study Journal Article United States J Matern Fetal Neonatal Med. 2002 Oct;12(4):247-52.},
	number = {4},
	pages = {247-52},
	title = {Comparison of fetal growth estimates based on birth weight and ultrasound references},
	type = {Journal Article},
	url = {http://www.tandfonline.com/doi/abs/10.1080/jmf.12.4.247.252},
	volume = {12},
