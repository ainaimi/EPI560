	pages = {162--167},
	source = {Paediatr Perinat Epidemiol. 2015 Mar;29(2):162-7. doi: 10.1111/ppe.12177. Epub 2015 Feb 14.},
	title = {Recruitment for longitudinal, randomised pregnancy trials initiated preconception: lessons from the effects of aspirin in gestation and reproduction trial},
	volume = {29},
