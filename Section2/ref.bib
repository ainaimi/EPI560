@article{Robins1992,
	abstract = {Cox & Oakes (1984, p. 66) introduced the strong version' of the accelerated
	failure time model with time-dependent exposures. We provide conditions
	under which this model could be used to estimate, from observational
	data, the causal effect of a time-varying exposure or treatment on
	time to an event of interest in the presence of time-dependent confounding
