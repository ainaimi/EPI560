	journal = {J Am Stat Assoc},
	number = {331},
	owner = {ain},
	pages = {1071---1094},
	timestamp = {2013.02.10},
	title = {Balanced Repeated Replications for Standard Errors},
	volume = {65},
	year = {1970}}

@article{Klebanoff2008,
	abstract = {The authors attempted to catalog the use of procedures to impute missing
