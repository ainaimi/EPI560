	source = {J Perinatol. 2012 Jul;32(7):483-90. doi: 10.1038/jp.2011.193. Epub 2012 Mar 29.},
	title = {Second trimester maternal plasma levels of cytokines IL-1Ra, Il-6 and IL-10 and preterm birth},
	volume = {32},
