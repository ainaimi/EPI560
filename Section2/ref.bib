	et = {2009/07/16},
	id = {9066},
	isbn = {1471-2458 (Electronic) 1471-2458 (Linking)},
	j2 = {BMC public health},
	keywords = {Adult Educational Status Emigrants and Immigrants/*statistics & numerical data Female *Health Status Disparities Humans *Income Infant, Newborn *Infant, Small for Gestational Age Logistic Models Male Mothers Odds Ratio *Population Density Premature Birth/*epidemiology Quebec/epidemiology Residence Characteristics Social Class Young Adult},
	la = {eng},
