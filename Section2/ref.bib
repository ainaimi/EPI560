	number = {3},
	owner = {epiduser},
	pages = {779-788},
	timestamp = {2012.05.14},
	title = {Correcting for Noncompliance and Dependent Censoring in an AIDS Clinical Trial with Inverse Probability of Censoring Weighted (IPCW) Log-Rank Tests},
	volume = {56},
	year = {2000}}

