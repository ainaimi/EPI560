	j1 = {Milbank Q},
	journal = {The Milbank Quarterly},
	month = {12},
	number = {4},
	pages = {616--622},
	publisher = {Blackwell Publishing Inc},
	title = {On the Choice of Absolute or Relative Inequality Measures},
	ty = {JOUR},
	u1 = {21166871{$[$}pmid{$]$}; 21166871{$[$}pmid{$]$}},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC3037178/},
	volume = {88},
