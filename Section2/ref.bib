@article{Liang1986,
	abstract = {This paper proposes an extension of generalized linear models to the
	analysis of longitudinal data. We introduce a class of estimating
	equations that give consistent estimates of the regression parameters
	and of their variance under mild assumptions about the time dependence.
	The estimating equations are derived without specifying the joint
	distribution of a subject's observations yet they reduce to the score
	equations for niultivariate Gaussian outcomes. Asymptotic theory
	is presented for the general class of estimators. Specific cases
