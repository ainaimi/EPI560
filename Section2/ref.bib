	j2 = {European journal of epidemiology},
	keywords = {Abortion, Induced Adult Asia/ethnology *Asian Continental Ancestry Group Female Humans Logistic Models Male Pregnancy Prejudice Quebec Retrospective Studies *Sex Preselection *Sex Ratio Young Adult},
	la = {eng},
