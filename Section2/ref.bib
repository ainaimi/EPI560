	journal = {Am J Epidemiol},
	number = {6},
	owner = {epiduser},
	pages = {691-700},
	timestamp = {2012.06.14},
	title = {Time Scale and Adjusted Survival Curves for Marginal Structural Cox Models},
	volume = {171},
	year = {2010}}

