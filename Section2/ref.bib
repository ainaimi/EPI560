@article{Greenland1994,
	abstract = {Many authors have pointed out that relative-risk estimates derived
	from ecologic data are vulnerable to biases not found in estimates
	derived from individual-level data. Nevertheless, biases in ecologic
	studies still are often dealt with in the same manner as biases in
	other observational studies, and so are not given adequate treatment.
