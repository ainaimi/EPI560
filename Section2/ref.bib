@article{Vinokur1997,
	abstract = {Earlier analyses of the JOBS II intervention for unemployed job seekers demonstrated that the intervention facilitated reemployment, reduced depressive symptoms, and improved role and emotional functioning (A. D. Vinokur, R. H. Price, & Y. Schul, 1995). The present study focuses on mediational processes of the active ingredients targeted by the intervention. Structural equation modeling analysis demonstrated that an enhanced sense of mastery had mediating effects of reemployment, financial strain, and reduction in depressive symptoms. Another active ingredient, inoculation against setbacks, was shown to protect those who suffered the setback of losing a job after temporarily regaining one. The inoculation protected them from experiencing the high level of depressive symptoms that was typical of their counterparts in the control group.},
	author = {Vinokur, A D and Schul, Y},
	date-added = {2016-01-06 12:24:20 +0000},
