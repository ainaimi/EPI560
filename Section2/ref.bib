@article{Westreich2010a,
	abstract = {Positivity, or the experimental treatment assignment assumption, requires
	that there be both exposed and unexposed participants at every combination
	of the values of the observed confounders in the population under
	study. Positivity is essential for inference but is often overlooked
	in practice by epidemiologists. This issue of the Journal includes
	2 articles featuring discussions related to positivity. Here the
	authors define positivity, distinguish between deterministic and
	random positivity, and discuss the 2 relevant papers in this issue.
	In addition, the commentators illustrate positivity in simple 2 x
	2 tables, as well as detail some ways in which epidemiologists may
	examine their data for nonpositivity and deal with violations of
	positivity in practice.},
	author = {Westreich, Daniel and Cole, Stephen R.},
	date-added = {2015-06-27 00:36:44 +0000},
