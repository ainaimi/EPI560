	journal = {Am J Reprod Immunol},
	keywords = {Dendritic cells; implantation; inflammation; pregnancy},
	month = {Aug},
	number = {2},
	oto = {NOTNLM},
	pages = {141--147},
	title = {The role of inflammation for a successful implantation},
	volume = {72},
	year = {2014},
